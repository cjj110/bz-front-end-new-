.score_detail {
  margin: 20rpx;
  background: #fff;
  font-size: 28rpx;
  color: #666;
  border-bottom: 2rpx solid #e5e5e5;
  padding: 0 20rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-radius:10rpx; 
}

.score_detail_z image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 10rpx
}

.score_detail>view {
}

.score_detail>view:nth-child(1) {
  text-align: left;
}

.score_detail>view:nth-child(2) {
  text-align: right;
  width: 20%;
}
