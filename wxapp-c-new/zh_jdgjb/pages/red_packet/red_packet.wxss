}

.order_title {
  display: flex;
  padding: 26rpx 40rpx;
}

.order_title .order_tabClick {
  background: #ec6464;
  color: #fff;
}

.order_title>view {
  width: 214rpx;
  text-align: center;
  line-height: 70rpx;
  height: 70rpx;
  font-size: 28rpx;
  box-sizing: border-box;
  color: #ec6464;
  border-radius: 10rpx;
  background: #fff;
  border: 1rpx solid #f1f1f1;
}

.address1 {
  background: #fff;
  width: 670rpx;
  margin: 0 auto;
  border-radius: 16rpx;
  /* height: 310rpx; */
  padding: 40rpx 0;
}

.address_one {
  text-align: center;
  font-size: 24rpx;
}

.address_one > view:nth-child(1) {
  margin: 30rpx 0;
}

.address_one2 {
  text-align: left;
}

.address_one2 {
  margin: 0 auto;
  width: 600rpx;
  padding: 30rpx 0;
  padding-bottom: 45rpx;
}

.address_one2 >text:nth-child(1) {
  padding-right: 30rpx;
}

.address_one3 {
  background: #fff;
  padding: 30rpx 0;
  width: 400rpx;
  height: 100rpx;
  margin: 0 auto;
  word-wrap: break-word;
