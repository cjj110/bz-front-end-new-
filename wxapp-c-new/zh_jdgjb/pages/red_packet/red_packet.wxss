  margin: 0 10rpx;
}

.btt {
  width: 380rpx;
  background: #ec6464;
  /* border-radius: 16rpx; */
  margin: 0 auto;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  margin-top: 60rpx;
  font-size: 24rpx;
  color: #fff;
