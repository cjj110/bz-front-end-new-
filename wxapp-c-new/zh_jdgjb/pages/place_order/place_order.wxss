page {
  width: 100%;
  height: 100%;
}

.container {
  width: 100%;
  height: 100%;
  padding-bottom: 100rpx;
}

.hotel {
  width: 94.7%;
  height: 290rpx;
  box-sizing: border-box;
  margin: 0 auto;
  background: #fff;
  border-radius: 16rpx;
  margin-top: 30rpx;
  padding: 40rpx 22rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;

}

.tip {
  font-size: 22rpx;
  margin-left: 68rpx;
  color: #919191;
  margin-top: 23rpx;
  margin-bottom: 26rpx;
}

.hotel>view {
  width: 90%;
  margin-left: 5%;
}

.hotel_name {
  height: 90rpx;
  font-size: 32rpx;
  color: #000;
  font-weight: 600;
  word-wrap:break-word;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2;
}

.hotel_info {
  height: 60rpx;
  line-height: 55 rpx;
  font-size: 24rpx;
  color: #666;
}

.hotel_info image {
  width: 30rpx;
  height: 35rpx;
  display: block;
  float: left;
  padding: 5rpx 0 0 0;
}

.hotel_info view {
  float: left;
  width: 90%;
  text-indent: 8rpx;
}

.hotel_time {
  overflow: hidden;
}

.hotel_time>view {
  width: 40%;
  display: inline-block;
}

.hotel_time>view text {
  color: #182e8f;
}

.pople_icon {
  font-size: 30rpx;
  color: #ec6464;
  margin-left: 10rpx;
}

.tel image {
  width: 28rpx;
  height: 28rpx;
}

.form_info {
  font-size: 24rpx;

  width: 94.7%;
  height: auto;
  background: #fff;
  margin: 0 auto;
  border-radius: 16rpx;
}

.fomr_tie {
  width: 80%;
  line-height: 36rpx;
  /* margin-left: 68rpx;
  margin-right: 170rpx;
  margin-top: 52rpx; */
  margin: 30rpx auto;
  font-size: 24rpx;
  color: #ec6464;
}

.fome_name {
  color: #ec6464;
  font-size: 26rpx;
}

.fomr_dd {
  margin: 0 auto;  
  font-size: 24rpx;
  width: 80%;
  color: #919191;
}

.icon-add1 {
  color: #ec6464;
  font-size: 40rpx;
  margin-left: 10rpx;
}

.form_info>view {
  /* width: 95%; */
  margin: 0 30rpx;
}

.form_info_child {
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 2rpx solid #f9f9f9;
  box-sizing: border-box;
  overflow: hidden;
}
.form_info_child:last-child{
  border: none;
}

.form_info_child>view {
  float: left;
}

.form_info_child>view:nth-child(1) {
  width: 30%;
  color: #777;
}

.form_info_child>view:nth-child(2) {
  width: 70%;
}

.form_info_child>view input {
  width: 100%;
  height: 60rpx;
  margin-top: 20rpx;
}

.home_num>view:nth-child(1) {
  width: 30%;
  height: 100%;
}

.home_num>view:nth-child(2) {
  width: 40%;
  height: 100%;
}

.home_num>view:nth-child(3) {
  width: 30%;
  height: 100%;
}

.home_num>view image {
  width: 50rpx;
  height: 50rpx;
  padding: 30rpx 0 0 0;
}

.home_num>view {
  float: left;
}

.bottom_price {
  width: 100%;
  height: 120rpx;
  position: fixed;
  z-index: 1000;
  bottom: 0;
  left: 0;
}

.bottom_price>view {
  float: left;
  height: 100%;
  line-height: 120rpx;
}

.heji>view:nth-child(1) {
  text-align: left;
  display: inline-block;
  vertical-align: middle;
  padding: 0 20rpx 0 40rpx;
}

.heji>view:nth-child(2) {
  text-align: left;
  display: inline-block;
  vertical-align: middle;
}

.bottom_price>view view:nth-child(2) {
  font-size: 24rpx;
}

.bottom_price>view:nth-child(1) {
  width: 55%;
  background: #fff;
  font-size: 24rpx;
}

.bottom_price>view:nth-child(2) {
  width: 45%;
  text-align: center;
  color: #fff;
  font-size: 35rpx;
}

.picker text {
  display: inline-block;
  width: 45%;
  height: 100%;
}

.picker text:last-child {
  text-align: right;
}

.button1 {
  width: 100%;
  height: 80rpx;
  background: none;
  line-height: 80rpx;
  display: inline-block;
  color: #fff;
  font-size: 32rpx;
  border-radius: 200rpx;
  margin: 0 20rpx;
}

.button2 {
  width: 100%;
  height: 100%;
  background: none;
  line-height: 50rpx;
  display: inline-block;
  border-radius: 0;
  color: #fff;
  font-size: 32rpx;
}

button::after {
  border: none;
}

.back {
  position: fixed;
  width: 100%;
  height: 100%;
  opacity: 0.5;
  top: 0;
  background: #000;
  z-index: 1000000;
}

.see_more {
  position: fixed;
  width: 90%;
  height: auto;
  max-height: 500rpx;
  z-index: 1000000000000;
  border-radius: 10rpx;
  overflow: hidden;
  background: #fff;
  overflow-x: hidden;
  overflow-y: scroll;
}

.see_more>view {
  padding: 0 20rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-bottom: 2rpx solid #f9f9f9;
  box-sizing: border-box;
}

.see_more>view view:nth-child(1) {
  float: left;
}

.see_more>view view:nth-child(2) {
  float: right;
}

.fixed {
}

.fixed>view:nth-child(1) {
  background: #ec6464;
  opacity: 0.5;
}

.pay_mode {
  width: 85%;
  height: auto;
  overflow: hidden;
  background: #fff;
  border-radius: 15rpx;
  margin-left: 10%;
  z-index: 50000000000;
  position: relative;
  margin: 0 auto;
  margin-top: 45%;
}

.pay_mode>view:nth-child(1) {
  /* text-align: center; *//* background: #e5e5e5; */
  color: #333;
  padding: 20rpx 0;
}

.mode {
  margin-bottom: 44rpx;
}

.pay_mode image {
  display: inline-block;
  vertical-align: middle;
  width: 50rpx;
  margin-right: 20rpx;
  height: 50rpx;
}

.pay_mode icon {
  display: inline-block;
  vertical-align: middle;
  width: 40rpx;
  right: -15rpx;
  height: 40rpx;
}

.btn {
  width: 300rpx;
  height: 80rpx;
  background: #ec6464;
  line-height: 80rpx;
  display: inline-block;
  color: #fff;
  font-size: 32rpx;
  border-radius: 200rpx;
  margin: 0 20rpx;
  text-align: center;
}

button::after {
  content: none;
  width: 200%;
  height: 200%;
  position: absolute;
  top: 0;
  left: 0;
  border: none;
  -webkit-transform: scale(0.5);
  transform: scale(0.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  box-sizing: border-box;
  border-radius: 10px;
}

.down {
  width: 100rpx;
}

.down image {
  width: 32rpx;
  height: 33rpx;
}

.down_hoice {
  background: #fff;
  /* width: 100%; */
  padding: 0 80rpx;
  font-size: 24rpx;
  margin-top: 20rpx;
}

.down_hoice image {
  width: 31rpx;
  height: 34rpx;
}

.down_tie {
  position: relative;
}

.down_h {
  padding: 20rpx 0;
  border-bottom: 1rpx solid #f1f1f1;
}
