.xing {
  width: 20rpx;
  height: 20rpx;
  margin-right: 10rpx;
  display: none;
}

.head .avtion {
  display: block;
}

.head {
  padding: 20rpx 30rpx;
  height: auto;
  /* background: #fff; */
}

.head textarea {
  width: 100%;
  height: 300rpx;
}

.head>view:nth-child(2) {
  background: #fff;
  margin-top: 30rpx;
}

.evaluate_number {
  background: #fff;
  margin: 0 auto;
  padding: 20rpx;
  border-radius: 8rpx;
}

.text_textarea {
  background: #fff;
  margin: 0 auto;
  padding: 10rpx;
  margin-top: 20rpx;
  border-radius: 8rpx;
}

.evaluate_img {
  width: 25%;
  overflow: hidden;
  height: 180rpx;
  box-sizing: border-box;
  border-right: 2rpx solid #f9f9f9;
  display: inline-block;
  vertical-align: middle;
  margin-bottom: 20rpx;
  position: relative;
  margin-left: 20rpx;
}

.delete {
  top: 0;
  width: 200rpx;
  height: 200rpx;
  right: 0;
}

.delete image {
  height: 50rpx;
  width: 50rpx;
  right: 0;
}

/* .head>view view view {
  width: 100%;
  height: 50rpx;
  bottom: 0;
  left: 0;
  border: none;
} */

.head>view view view view:nth-child(1) {
  width: 100%;
  height: 100%;
  background: #000;
  z-index: 100;
  opacity: 0.5;
  top: 0;
  border: none;
}

.head>view view view view:nth-child(2) {
  width: 100%;
  height: 100%;
  z-index: 10000;
  top: 0;
  border: none;
}

.head>view view view view image {
  width: 40rpx;
  height: 40rpx;
}

.add_img {
  /* border: 4rpx solid rgb(122, 122, 122); */
  border-radius: 10rpx;
  /* width: 25%; */
  overflow: hidden;
  height: 180rpx;
  box-sizing: border-box;
  display: inline-block;
  vertical-align: middle;
  margin-bottom: 20rpx;
  text-align: center;
  color: #999;
  font-size: 20rpx;
  margin-left: 20rpx;
}

.evaluate {
  width: 200rpx;
  height: 200rpx;
}

.add_img image {
  width: 130rpx;
  height: 130rpx;
}

.head>view:last-child {
}

.abb {
  text-align: right;
  color: #febfb9;
}

button {
  background: #ec6464;
  width: 90%;
  color: #fff;
  margin-top: 50rpx;
  margin-bottom: 50rpx;
  border-radius: 50rpx;
}

.success_name {
  margin: 20rpx 40rpx;
}

.icon-xingxing {
  color: #999;
  margin-right: 16rpx;
}

page .cur {
  color: #ec6464;
}
